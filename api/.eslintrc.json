{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": ["plugin:prettier/recommended", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}